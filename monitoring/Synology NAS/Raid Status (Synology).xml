		<thresholddefaults>
			<thresholds configurable="false" type="String - Huge"/>
		</thresholddefaults>
		<description country="ca" language="en">RAID Name</description>
		<help country="ca" language="en">Help</help>
		<popuphelp country="ca" language="en">Popup Help</popuphelp>
		<displayname country="ca" language="en">SNMP18993_2</displayname>
	</scandetail>



