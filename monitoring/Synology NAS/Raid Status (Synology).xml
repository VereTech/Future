<service xmlns="http://www.n-able.com" author="N-able SDK" creationdate="12/21/2012" organization="N-able Technologies" syntaxversion="1.1.0.0" version="1.0.0.4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">


	<servicedefinition id="18993" reason="" releasedependency="4.5.0.0" servicetype="SNMP" version="1.0.0.4">
		<description country="ca" language="en">System status service for Synology NAS devices.  Must be running DSM 4.1 or greater.</description>
		<displayname country="ca" language="en">RAID Status &#40;Synology&#41;</displayname>
		<displaylabel>RAID Status &#40;Synology&#41;</displaylabel>
		<help country="ca" language="en">Custom SNMP Service</help>
		<popuphelp country="ca" language="en">Custom SNMP Service</popuphelp>


		<serviceparameters aggregatedata="true" aggregatetasks="false" maxinstances="10" maxpollrate="59" minpollrate="1" schedulertype="Cron-like Scheduler" serviceinstancetype="Single" timetostale="10000"/>


		<moduleparameters>
