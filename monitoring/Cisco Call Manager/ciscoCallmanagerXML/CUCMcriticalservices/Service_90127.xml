            <longdescription country="ca" language="en">ccm process normal mapping</longdescription>
            <help country="ca" language="en">ccm process normal mapping</help>
          </moduleparameter>

         </moduleparameters>
 
      <scandetails>
        <scandetailid>90127</scandetailid>
      </scandetails>

