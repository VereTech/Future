<service xmlns="http://www.n-able.com" author="N-able SDK" creationdate="9/23/2010" organization="N-able Technologies" syntaxversion="1.1.0.0" version="1.0.0.4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">


	<servicedefinition id="24175" reason="" releasedependency="4.5.0.0" servicetype="SNMP" version="1.0.0.4">
		<description country="ca" language="en">Using SNMP, this service queries the current &#39;alarm level&#39; of the Mitel MCD device.</description>
		<displayname country="ca" language="en">Mitel MCD Alarm Level</displayname>
		<displaylabel>Mitel MCD Alarm Level</displaylabel>
		<help country="ca" language="en">Custom SNMP Service</help>
		<popuphelp country="ca" language="en">Custom SNMP Service</popuphelp>


		<serviceparameters aggregatedata="true" aggregatetasks="false" maxinstances="1" maxpollrate="1440" minpollrate="1" schedulertype="Interval Based Scheduler" serviceinstancetype="Single" timetostale="30"/>


		<moduleparameters>
			<moduleparameter key="scan_interval" max="1440" min="1" phardcoded="false" preferredident="false" type="Integer" value="1">
				<shortdescription country="ca" language="en">Scan Interval</shortdescription>
				<longdescription country="ca" language="en">Scan Interval</longdescription>
				<help country="ca" language="en">The number of minutes between scans.</help>
				<presentationinfo>
					<presentationitem datatype="String" description="" type="mandatory" value="" />
					<presentationitem datatype="String" description="" type="validation" value="^[0-9]+$" />
				</presentationinfo>
			</moduleparameter>



			<moduleparameter key="Group.0.TargetIndex.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="0">
				<shortdescription country="ca" language="en">SNMP Index</shortdescription>
				<longdescription country="ca" language="en">SNMP Index</longdescription>
				<help country="ca" language="en">Enter the name of the SNMP Index to be used by this service</help>
				<presentationinfo>
					<presentationitem datatype="String" description="" type="mandatory" value="" />
					<presentationitem datatype="String" description="" type="validation" value="^[0-9]+([.][0-9]+)*$" />
				</presentationinfo>
			</moduleparameter>
			<moduleparameter key="Group.0.Var.0.VarName.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="AlarmLevel">
				<shortdescription country="ca" language="en">Variable 0</shortdescription>
				<longdescription country="ca" language="en">Variable 0</longdescription>
				<help country="ca" language="en">Variable 0</help>
			</moduleparameter>
			<moduleparameter key="Group.0.Var.0.OID.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="1.3.6.1.4.1.1027.4.1.1.2.2.1">
				<shortdescription country="ca" language="en">OID 0</shortdescription>
				<longdescription country="ca" language="en">OID 0</longdescription>
				<help country="ca" language="en">OID 0</help>
			</moduleparameter>




			<moduleparameter key="Scandetail.0.ScandetailName.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="SNMP24175_1">
				<shortdescription country="ca" language="en">Scan detail name</shortdescription>
				<longdescription country="ca" language="en">Scan detail name</longdescription>
				<help country="ca" language="en">Scan detail name</help>
			</moduleparameter>
			<moduleparameter key="Scandetail.0.Recipe.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="$AlarmLevel">
				<shortdescription country="ca" language="en">Scan detail recipe</shortdescription>
				<longdescription country="ca" language="en">Scan detail recipe</longdescription>
				<help country="ca" language="en">Scan detail recipe</help>
			</moduleparameter>
			<moduleparameter key="Scandetail.0.Type.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="UINT8">
				<shortdescription country="ca" language="en">Scan detail type</shortdescription>
				<longdescription country="ca" language="en">Scan detail type</longdescription>
				<help country="ca" language="en">Scan detail type</help>
			</moduleparameter>




			<moduleparameter key="Scandetail.1.ScandetailName.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="SNMP24175_2">
				<shortdescription country="ca" language="en">Scan detail name</shortdescription>
				<longdescription country="ca" language="en">Scan detail name</longdescription>
				<help country="ca" language="en">Scan detail name</help>
			</moduleparameter>
			<moduleparameter key="Scandetail.1.Recipe.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="$AlarmLevel">
				<shortdescription country="ca" language="en">Scan detail recipe</shortdescription>
				<longdescription country="ca" language="en">Scan detail recipe</longdescription>
				<help country="ca" language="en">Scan detail recipe</help>
			</moduleparameter>
			<moduleparameter key="Scandetail.1.Type.0" max="" min="" phardcoded="true" preferredident="false" type="String" value="VERYLONGSTRING">
				<shortdescription country="ca" language="en">Scan detail type</shortdescription>
				<longdescription country="ca" language="en">Scan detail type</longdescription>
				<help country="ca" language="en">Scan detail type</help>
			</moduleparameter>

			<moduleparameter key="Scandetail.1.Map.No Active Alarms" max="" min="" phardcoded="true" preferredident="false" type="String" value="1">
				<shortdescription country="ca" language="en">Custom Mapping</shortdescription>
				<longdescription country="ca" language="en"/>
				<help country="ca" language="en"/>
			</moduleparameter>
			<moduleparameter key="Scandetail.1.Map.Minor Alarm" max="" min="" phardcoded="true" preferredident="false" type="String" value="2">
				<shortdescription country="ca" language="en">Custom Mapping</shortdescription>
				<longdescription country="ca" language="en"/>
				<help country="ca" language="en"/>
			</moduleparameter>
			<moduleparameter key="Scandetail.1.Map.Major Alarm" max="" min="" phardcoded="true" preferredident="false" type="String" value="3">
				<shortdescription country="ca" language="en">Custom Mapping</shortdescription>
				<longdescription country="ca" language="en"/>
				<help country="ca" language="en"/>
			</moduleparameter>
			<moduleparameter key="Scandetail.1.Map.Critical Alarm" max="" min="" phardcoded="true" preferredident="false" type="String" value="4">
				<shortdescription country="ca" language="en">Custom Mapping</shortdescription>
				<longdescription country="ca" language="en"/>
				<help country="ca" language="en"/>
			</moduleparameter>

		</moduleparameters>



		<scandetails>
			<scandetailid>2417501</scandetailid>
			<scandetailid>2417502</scandetailid>
		</scandetails>
	</servicedefinition>



	<scandetail id="2417501" monitoringtypeon="true" processforstate="true" releasedependency="4.5.0.0" version="1.0.0.0" >
		<thresholddefaults>
			<thresholds configurable="true" type="uint8">
				<threshold high="1" low="1" state="Normal"/>
				<threshold high="2" low="2" state="Warning"/>
				<threshold high="4" low="3" state="Failed"/>
			</thresholds>
		</thresholddefaults>
		<description country="ca" language="en">Alarm Level</description>
		<help country="ca" language="en">Help</help>
